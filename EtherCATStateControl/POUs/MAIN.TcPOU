	EIPDataIn AT %I*		: ARRAY[1..5] OF WORD; 
	EIPDataOut AT %Q*		: ARRAY[1..5] OF WORD; 

END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[IF bSetToInit THEN
	IF fbDeviceStateCtrl.SetState(1,T#2S) THEN
		bSetToInit := FALSE;
	END_IF
END_IF

